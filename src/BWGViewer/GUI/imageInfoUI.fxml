<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="300.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="BWGViewer.GUI.ImageInfoUI">
    <GridPane alignment="CENTER_LEFT" prefHeight="300.0" prefWidth="240.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="80.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="120.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Label fx:id="nameText" text="Name"/>
        <Label fx:id="typeText" text="Type" GridPane.rowIndex="1"/>
        <Label fx:id="widthText" text="Width" GridPane.rowIndex="2"/>
        <Label fx:id="heightText" text="Height" GridPane.rowIndex="3"/>
        <Label fx:id="bitsText" text="Bits" GridPane.rowIndex="4"/>
        <Label fx:id="creationTimeText" text="CreationTime" GridPane.rowIndex="5"/>
        <Label fx:id="modifiedTimeText" text="ModifiedTime" GridPane.rowIndex="6"/>
        <Label fx:id="sizeText" text="Size" GridPane.rowIndex="7"/>
        <Label fx:id="name" text="Label" GridPane.columnIndex="1"/>
        <Label fx:id="type" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label fx:id="width" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label fx:id="height" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Label fx:id="bits" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <Label fx:id="creationTime" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <Label fx:id="modifiedTime" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <Label fx:id="size" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
    </GridPane>
</AnchorPane>
