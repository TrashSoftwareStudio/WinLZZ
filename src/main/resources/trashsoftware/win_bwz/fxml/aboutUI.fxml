<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="180.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml" fx:controller="trashsoftware.win_bwz.gui.controllers.AboutUI">
    <VBox alignment="CENTER" prefHeight="240.0" prefWidth="180.0" spacing="10.0">
        <HBox alignment="CENTER" prefHeight="40.0" prefWidth="180.0" spacing="10.0">
            <Label text="WinLZZ">
                <font>
                    <Font size="18.0"/>
                </font>
            </Label>
            <Label fx:id="versionLabel">
                <font>
                    <Font size="18.0"/>
                </font>
            </Label>
        </HBox>
        <Label fx:id="trashSoftwareLabel" text="%trashSoftwareStudio"/>
        <Label text="2017-2018"/>
        <Label fx:id="coreVersionTextLabel" text="%coreVersion"/>
        <Label fx:id="coreVersionLabel"/>
    </VBox>
</AnchorPane>
