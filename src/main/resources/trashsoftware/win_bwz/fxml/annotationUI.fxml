<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="trashsoftware.win_bwz.GUI.AnnotationUI">
    <VBox alignment="CENTER_LEFT" prefHeight="300.0" prefWidth="400.0" spacing="5.0">
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
        <Label fx:id="fromFile" text="文件注释"/>
        <HBox alignment="CENTER" prefHeight="40.0" prefWidth="400.0" spacing="5.0">
            <ComboBox fx:id="fileBox" prefWidth="300.0"/>
            <Button fx:id="browseButton" mnemonicParsing="false" onAction="#browseAction" prefWidth="80.0" text="浏览"/>
        </HBox>
        <Label fx:id="fromText" text="手动注释"/>
        <TextArea fx:id="textArea" prefHeight="160.0" prefWidth="200.0"/>
        <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" spacing="10.0">
            <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#confirmAction" text="确认"/>
            <CheckBox fx:id="compressAnnBox" mnemonicParsing="false" text="压缩注释"/>
            <Label fx:id="warning"/>
        </HBox>
    </VBox>
</AnchorPane>
